
<h2>{{title}}</h2>
<hr/>
<div class="table-responsive">
    <table class="table table-bordered table-striped table-hover">
        <thead>
            <tr>
                <th scope="col">ID</th>
                <th scope="col">Email</th>
                <th scope="col">Password</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
            {{#each data}}
            <tr class="">
                <td>{{this._id}}</td>
                <td>{{this.email}}</td>
                <td>{{this.password}}</td>
                <td>
                    <a type="button" class="btn btn-success me-2" href="/form/user/edit/{{this._id}}"><i class="bi bi-pen"></i></a>
                    <a type="button" class="btn btn-danger" href="/form/user/delete/{{this._id}}"><i class="bi bi-trash"></i></a>
               
                </td>
            </tr>
            {{/each}}
        </tbody>
    </table>
</div>
